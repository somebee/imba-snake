{
  "name": "imba-snake",
  "description": "Snake Game in imba",
  "keywords": [
    "imba",
    "sass"
  ],
  "author": "Tomasz Wegrzanowski",
  "version": "1.0.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "dev": "webpack-dev-server --mode=development --content-base dist/",
    "build": "imbac --standalone -o dist/ src/",
    "watch": "imbac --standalone -w -o dist/ src/",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/taw/imba-snake.git"
  },
  "license": "MIT",
  "dependencies": {
    "imba": "^2.0.0-alpha.12",
    "webpack-dev-server": "^3.7.1"
  }
}
